<h3> Available Categories </h3>
<p><%= link_to "Add Category", new_category_path(@categories) %></p>
<% @categories.each do |category| %>
  <p><%= link_to "#{category.title}", category_path(category) %><p>
    <%= button_to "Delete", category_path(category), method: "delete" %>
    <%= button_to "Edit", edit_category_path(category) %>
<% end %>
